{
  "name": "meants-starter",
  "version": "0.0.0",
  "description": "MEAN stack development with Angular and Express, both powered by Typescript.",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "update:packages": "ncu -u && ncu -a && npm update",
    "ng": "ng",
    "start": "npm-run-all -s clean build start:simple",
    "start:simple": "node .",
    "start:dev": "npm-run-all -s clean build -p watch:*",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:cov": "rimraf coverage",
    "prebuild": "npm rebuild node-sass",
    "build": "npm-run-all -s build:ts build:swagger -p build:frontend",
    "build:ts": "tsc -p src/backend/tsconfig.json",
    "build:swagger": "node ./dist/backend/tasks/swagger.js",
    "build:frontend": "ng build",
    "watch:ts": "tsc -p src/backend/tsconfig.json --watch",
    "lint": "ng lint && tslint \"src/backend/**/*.ts\" --project src/backend/tsconfig.json",
    "test": "ng test",
    "e2e": "ng e2e"
  },
  "private": false,
  "author": "ian.raffin@gmail.com",
  "keywords": [
    "Express",
    "Typescript",
    "Angular",
